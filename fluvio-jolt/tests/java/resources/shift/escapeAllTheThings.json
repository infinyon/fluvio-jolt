{
    "input": {
        "@context": "atSymbol",
        "$name": "Mojito",
        "&ingredient": "mint",
        "[yield": "open array",
        "[]yield": "full array",
        "]yield": "back array",
        "*" : "star",
        "#" : "hash",
        "(" : "left paren"
    },

    "spec": {
        // TEST escaping all the things when they are the first char(s) of the spec LHS and RHS
        "\\@context": "\\@A",
        "\\$name": "\\$B",
        "\\&ingredient": "\\&C",
        "\\[yield": "\\[D",
        "\\[\\]yield": "\\[\\]E",
        "\\]yield": "\\]F",
        "\\*" : "\\*G",
        "\\#" : "\\#H"
    },

    "expected": {
        "@A" : "atSymbol",
        "$B" : "Mojito",
        "&C" : "mint",
        "[D": "open array",
        "[]E" : "full array",
        "]F" : "back array",
        "*G" : "star",
        "#H" : "hash"
    }
}
